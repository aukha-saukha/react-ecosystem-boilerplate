{
  "dependencies": {
    "@loadable/component": "^5.12.0",
    "@loadable/server": "^5.12.0",
    "express": "^4.17.1",
    "morgan": "^1.9.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-router-dom": "^5.1.2",
    "uuid": "^7.0.2",
    "winston": "^3.2.1"
  },
  "description": "React ecosystem boilerplate",
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-flow": "^7.7.4",
    "@babel/preset-react": "^7.8.3",
    "@hot-loader/react-dom": "^16.13.0",
    "@loadable/babel-plugin": "^5.12.0",
    "@loadable/webpack-plugin": "^5.12.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "cypress": "^4.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.4.0",
    "flow-bin": "^0.120.1",
    "husky": "^4.2.3",
    "identity-obj-proxy": "^3.0.0",
    "inspectpack": "^4.4.0",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.8",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^1.19.1",
    "react-hot-loader": "^4.12.20",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "stylelint": "^13.2.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-order": "^4.0.0",
    "terser-webpack-plugin": "^2.3.5",
    "webpack": "^4.42.0",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-bundle-size-analyzer": "^3.1.0",
    "webpack-cli": "^3.3.11",
    "webpack-dashboard": "^3.2.0",
    "webpack-dev-server": "^3.10.3",
    "workbox-cacheable-response": "^5.0.0",
    "workbox-core": "^5.0.0",
    "workbox-expiration": "^5.0.0",
    "workbox-precaching": "^5.0.0",
    "workbox-routing": "^5.0.0",
    "workbox-strategies": "^5.0.0",
    "workbox-webpack-plugin": "^5.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn run flow && yarn run jest:related",
      "pre-pushed": "lint-staged && yarn run flow && yarn run jest:related"
    }
  },
  "license": "MIT",
  "lint-staged": {
    "*.{html,md}": [
      "prettier -c"
    ],
    "src/**/*.(c|sc)ss": "stylelint",
    "src/**/*.js": "eslint",
    "tools/**/*.js": "eslint"
  },
  "name": "react-ecosystem-boilerplate",
  "repository": "https://github.com/aukha-saukha/react-ecosystem-boilerplate",
  "scripts": {
    "build:dev": "yarn run build:dev:client && yarn run build:dev:server",
    "build:dev:client": "webpack --config tools/webpack/client.dev.config.js",
    "build:dev:server": "webpack --config tools/webpack/server.dev.config.js",
    "build:prod": "yarn run build:prod:client && yarn run build:prod:server",
    "build:prod:client": "webpack --config tools/webpack/client.prod.config.js",
    "build:prod:server": "webpack --config tools/webpack/server.prod.config.js",
    "clean:dev": "rm -rf dist/dev",
    "clean:prod": "rm -rf dist/prod",
    "cy:open:dev": "cypress open --config-file test/cypress/config.json --env testEnvName=development",
    "cy:open:prod": "cypress open --config-file test/cypress/config.json --env testEnvName=production",
    "cy:run:dev": "cypress run --config-file test/cypress/config.json --env testEnvName=development",
    "cy:run:prod": "cypress run --config-file test/cypress/config.json --env testEnvName=production",
    "eslint": "eslint 'src/**/*.js' && eslint 'tools/**/*.js'",
    "flow": "flow",
    "flow:coverage": "flow batch-coverage src/",
    "jest": "jest --config test/jest/config.js --no-cache",
    "jest:coverage": "yarn jest --colors --coverage",
    "jest:related": "yarn jest:coverage --bail --changedSince=master",
    "jest:watch": "yarn jest --watch",
    "lint": "yarn run eslint && yarn run stylelint",
    "start:dev": "yarn run clean:dev && yarn run build:dev && node dist/dev/private/js/server.js",
    "start:prod": "yarn run clean:prod && yarn run build:prod && node dist/prod/private/js/server.js",
    "stylelint": "stylelint src/**/*.scss",
    "wds": "webpack-dashboard -m -- webpack-dev-server --config tools/webpack/dev-server.config.js"
  },
  "version": "1.0.12"
}
