[ignore]

[include]

[libs]
./flow-typed
module.name_mapper.extension='scss' -> 'empty/object'
module.system=haste

[lints]
ambiguous-object-type=warn
deprecated-type=warn
deprecated-utility=warn
; dynamic-export=warn
implicit-inexact-object=warn
nonstrict-import=warn
sketchy-null=warn
sketchy-number=warn
unclear-type=warn
unnecessary-invariant=warn
unnecessary-optional-chain=warn
unsafe-getters-setters=warn
; untyped-import=warn
untyped-type-import=warn

[options]
emoji=true
include_warnings=true
module.file_ext=.scss
module.file_ext=.js
module.file_ext=.json
module.ignore_non_literal_requires=true
module.name_mapper='^@client-utilities' -> '<PROJECT_ROOT>/src/client/utilities'
module.name_mapper='^@constants' -> '<PROJECT_ROOT>/src/data/constants'
module.name_mapper='^@hooks' -> '<PROJECT_ROOT>/src/client/hooks'
module.name_mapper='^@routes' -> '<PROJECT_ROOT>/src/shared/routes'
module.name_mapper='^@server-utilities' -> '<PROJECT_ROOT>/src/server/utilities'
module.name_mapper='^@shared-utilities' -> '<PROJECT_ROOT>/src/shared/utilities'
module.name_mapper='^@static' -> '<PROJECT_ROOT>/src/static'
module.name_mapper='^@translations' -> '<PROJECT_ROOT>/src/data/translations'
module.name_mapper='^@views' -> '<PROJECT_ROOT>/src/shared/views'
suppress_comment= \\(.\\|\n\\)*\\flow-disable-line

[strict]
nonstrict-import
unclear-type
unsafe-getters-setters
; untyped-import
untyped-type-import
